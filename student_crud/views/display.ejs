<a href="/add">Add Record</a>
<% if(db_rows_array.length > 0){ %> 

    <table border="1">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Physics</th>
            <th>Chemistry</th>
            <th>Maths</th>
            <th>Biology</th>
        </tr>
        <% for(var i=0;i<db_rows_array.length;i++){ %>
        <tr>
            <td><%= db_rows_array[i].s_id %> </td>
            <td><%= db_rows_array[i].name %> </td>
            <td><%= db_rows_array[i].physics %> </td>
            <td><%= db_rows_array[i].chemistry %> </td>
            <td><%= db_rows_array[i].maths %> </td>
            <td><%= db_rows_array[i].biology %> </td>
            <td><a href="/edit/<%= db_rows_array[i].s_id %>">Edit</a> | <a href="/delete/<%= db_rows_array[i].s_id %>">Delete</a></td>
        </tr>
        <% } %> 
    </table>

    <% }else{ %> 
        No records Found
        <% } %> 